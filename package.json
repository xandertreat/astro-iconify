{
  "author": "Xander Treat",
  "name": "@xtreat/astro-iconify",
  "version": "1.2.5",
  "description": "A type-safe simple and easy-to-modify Iconify Icon component for Astro",
  "readme": "README.md",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xandertreat/astro-iconify.git"
  },
  "bugs": {
    "url": "https://github.com/xandertreat/astro-iconify/issues"
  },
  "homepage": "https://github.com/xandertreat/astro-iconify?tab=readme-ov-file#readme",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./src/types.d.ts"
    }
  },
  "dependencies": {
    "astro": ">2.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@types/node": "^22.10.7",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "astro check",
    "fmt": "prettier --write ."
  },
  "keywords": [
    "astro",
    "component",
    "astro-component",
    "icons",
    "iconify",
    "svg",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  }
}
